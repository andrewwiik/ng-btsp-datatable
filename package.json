{
  "name": "ng-btsp-datatable",
  "version": "1.0.0",
  "description": "An Angular 4 data table, with pagination, sorting, expandable rows etc. Bootstrap 4 version.",
  "keywords": [
    "angular",
    "angular5",
    "Angular 5",
    "ng2",
    "datatable",
    "data-table",
    "data table",
    "Bootstrap",
    "Bootstrap 4",
    "pagination"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/datatable.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/datatable.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:afermon/angular-4-data-table-bootstrap-4.git"
  },
  "peerDependencies": {
    "@angular/core": "5.2.8"
  },
  "author": "afermon <alex@xicre.com>",
  "license": "MIT",
  "devDependencies": {
    "@angular/compiler": "5.2.8",
    "@angular/compiler-cli": "5.2.8",
    "rollup": "0.56.5",
    "typescript": "2.7.2",
    "uglify-js": "3.3.13"
  },
  "dependencies": {
    "@angular/common": "5.2.8",
    "@angular/core": "5.2.8",
    "@angular/forms": "5.2.8",
    "@angular/platform-browser": "5.2.8",
    "@angular/platform-browser-dynamic": "5.2.8",
    "@angular/platform-server": "5.2.8",
    "@types/node": "~9.4.7",
    "npm": "^5.7.1",
    "rxjs": "5.5.6",
    "ts-node": "~5.0.1",
    "zone.js": "^0.8.12"
  },
  "false": {},
  "bugs": {
    "url": "https://github.com/afermon/angular-4-data-table-bootstrap-4/issues"
  },
  "homepage": "https://github.com/afermon/angular-4-data-table-bootstrap-4/"
}
