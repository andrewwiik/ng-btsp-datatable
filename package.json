{
  "name": "ng-btsp-datatable",
  "version": "6.0.0",
  "description": "An Angular data table, with pagination, sorting, expandable rows etc. Bootstrap 4 version.",
  "keywords": [
    "angular",
    "angular5",
    "Angular 5",
    "ng2",
    "datatable",
    "data-table",
    "data table",
    "Bootstrap",
    "Bootstrap 4",
    "pagination"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/datatable.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/datatable.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:softawarriors/ng-btsp-datatable.git"
  },
  "peerDependencies": {
    "@angular/core": "6.1.9"
  },
  "author": "afermon <alex@xicre.com>",
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "6.1.9",
    "@angular-devkit/build-angular": "^0.8.0",
    "@angular/compiler": "6.1.9",
    "@angular/compiler-cli": "6.1.9",
    "rollup": "^0.56.5",
    "typescript": "^2.7.2",
    "uglify-js": "^3.3.13"
  },
  "dependencies": {
    "@angular/common": "6.1.9",
    "@angular/cli": "^6.2.4",
    "@angular/core": "6.1.9",
    "@angular/forms": "6.1.9",
    "@angular/platform-browser": "6.1.9",
    "@angular/platform-browser-dynamic": "6.1.9",
    "@angular/platform-server": "6.1.9",
    "@types/node": "^10.11.4",
    "npm": "^5.7.1",
    "rxjs": "^6.0.0",
    "ts-node": "^5.0.1",
    "zone.js": "~0.8.26"
  },
  "false": {},
  "bugs": {
    "url": "https://github.com/softawarriors/ng-btsp-datatable/issues"
  },
